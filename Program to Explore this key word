/*
 * Program to explore functions in Java and this key word.
 * This program is to find the area and volume of the Rectangle
 * @author:Jaya John
 */

package com.seamedu;

class Box{
	int length,breadth,height;
	void setData(int length,int breadth,int height) {
		this.length=length;
		this.breadth=breadth;
		this.height=height;
	}
	void findArea() {
		System.out.println("Area of a Reactangle: "+(length*breadth));
	}
	void findVolume() {
		System.out.println("Volume of a Reactangle: "+(length*breadth*height));
	}
	
}

public class Reactangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Box myBox=new Box();
		myBox.setData(10, 20, 30);
		myBox.findArea();
		myBox.findVolume();
	}

}
